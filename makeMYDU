CC = gcc
CFLAGS = -g
TARGET = mydu
OBJS = mydu.o depthfirstapply.o sizepathfun.o
$(TARGET): $(OBJS)
	$(CC) -o $(TARGET) $(OBJS)
mydu.o: mydu.c
	$(CC) $(CFLAGS) -c mydu.c
depthfirstapply.o: depthfirstapply.c
	$(CC) $(CFLAGS) -c depthfirstapply.c
sizepathfun.o: sizepathfun.c
	$(CC) $(CFLAGS) -c sizepathfun.c
clean:
	/bin/rm -f *.o $(TARGET)

